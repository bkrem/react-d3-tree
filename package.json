  },
  "homepage": "https://github.com/bkrem/react-d3-tree",
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "ava": "^0.12.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "coveralls": "^2.11.2",
